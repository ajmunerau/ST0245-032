public class Taller5{
    public static int[] insertionSort (int[] array){
    int n = array.length;  
    for (int j = 1; j < n; j++) {  
            int temp = array[j];  
            int i = j-1;  
            while ( (i > -1) && ( array [i] > temp ) ) {  
                array [i+1] = array [i];  
                i--;  
            }  
            array[i+1] = temp;  
        }  
    return array;
  }  
}
